{
    "type": "ee-add-on",
    "extra": {
        "handle": "executive",
        "systemPath": "src/executive",
        "version": "3.0.0-rc.1",
        "diConfigFilePath": "src/di/_collector.php"
    },
    "name": "buzzingpixel/executive-ee",
    "version": "3.0.0-rc.1",
    "description": "An ExpressionEngine toolkit that provides a CLI, migrations, command scheduling, queue processing, custom extensions, custom template tags, custom CP views, and more!",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-zip": "*",
        "buzzingpixel/corbomite-config-collector": "^1.1.2",
        "composer/composer": "^1.8",
        "filp/whoops": "^2.3",
        "php-di/php-di": "^6.0",
        "symfony/console": "^4.2",
        "symfony/filesystem": "^4.2",
        "symfony/finder": "^4.2",
        "twig/twig": "^2.7",
        "zendframework/zend-diactoros": "^2.1",
        "zendframework/zend-httphandlerrunner": "^1.1"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.1",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/dotenv": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "BuzzingPixel\\Executive\\": "src/executive",
            "buzzingpixel\\executive\\": "src/executive"
        },
        "classmap": [
            "src/executive/ext.executive.php",
            "src/executive/mcp.executive.php",
            "src/executive/mod.executive.php",
            "src/executive/upd.executive.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
