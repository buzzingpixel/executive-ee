{
    "name": "buzzingpixel/executive-ee",
    "version": "3.0.0-b.2",
    "description": "An ExpressionEngine toolkit that provides a CLI, migrations, command scheduling, custom extensions, custom template tags, custom CP views, and more!",
    "type": "ee-add-on",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:tjdraper/expressionengine.git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-zip": "*",
        "composer/composer": "^1.7",
        "php-di/php-di": "^6.0",
        "filp/whoops": "^2.2",
        "vlucas/phpdotenv": "^2.5",
        "symfony/console": "^4.1",
        "symfony/filesystem": "^4.1",
        "symfony/finder": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "tjdraper/expressionengine": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "BuzzingPixel\\Executive\\": "src/executive",
            "buzzingpixel\\executive\\": "src/executive"
        },
        "classmap": [
            "src/executive/ext.executive.php",
            "src/executive/mcp.executive.php",
            "src/executive/mod.executive.php",
            "src/executive/upd.executive.php"
        ]
    },
    "extra": {
        "handle": "executive",
        "systemPath": "src/executive"
    }
}
